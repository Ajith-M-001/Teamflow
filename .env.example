# Kinde OAuth Client ID (from your Kinde app)
KINDE_CLIENT_ID=your-client-id-here

# Kinde OAuth Client Secret (keep this private!)
KINDE_CLIENT_SECRET=your-client-secret-here

# Your Kinde tenant issuer URL (from Kinde dashboard)
KINDE_ISSUER_URL=https://your-tenant.kinde.com

# Your app/site URL (Next.js base URL)
KINDE_SITE_URL=http://localhost:3000

# Redirect URL after logout
KINDE_POST_LOGOUT_REDIRECT_URL=http://localhost:3000

# Redirect URL after login
KINDE_POST_LOGIN_REDIRECT_URL=http://localhost:3000/workspace


# Kinde Management API Configuration
KINDE_DOMAIN=mybusiness.kinde.com
KINDE_MANAGEMENT_CLIENT_ID=your_client_id_here
KINDE_MANAGEMENT_CLIENT_SECRET=your_client_secret_here

#arcjet
ARCJET_KEY=ajkey_yourkey

#database
DATABASE_URL="postgres://user:password@localhost:5432/mydb?schema=public"

#uploadthing
UPLOADTHING_TOKEN=your-uploadthing-token